{
  "name": "hacktabl",
  "version": "0.0.0",
  "description": "Comparison table generator, using Ethercalc and Google Doc as the content backend",
  "main": "index.html",
  "devDependencies": {
    "babel": "^5.8.34",
    "babel-core": "^5.8.34",
    "babel-loader": "^5.3.3",
    "babel-plugin-rewire": "^0.1.22",
    "babel-runtime": "^5.8.34",
    "bootstrap-sass": "^3.3.5",
    "chai": "^3.4.1",
    "chai-as-promised": "^5.1.0",
    "compass-mixins": "^0.12.7",
    "coveralls": "^2.11.4",
    "css-loader": "^0.15.6",
    "extract-text-webpack-plugin": "^0.8.2",
    "file-loader": "^0.8.4",
    "hacktabl-parser": "0.0.2",
    "history": "^1.13.1",
    "html-loader": "^0.3.0",
    "isomorphic-fetch": "^2.2.0",
    "istanbul": "git://github.com/gotwarlost/istanbul#source-map",
    "jade": "^1.11.0",
    "jade-html-loader": "0.0.2",
    "jsdom": "^7.0.2",
    "material-design-lite": "^1.0.6",
    "mocha": "^2.3.3",
    "mocha-lcov-reporter": "^1.0.0",
    "node-sass": "^3.2.0",
    "object-assign": "^4.0.1",
    "parallelshell": "^2.0.0",
    "raw-loader": "^0.5.1",
    "react": "^0.14.2",
    "react-redux": "^4.0.0",
    "react-router": "^1.0.0",
    "redux": "^3.0.4",
    "redux-devtools": "^2.1.5",
    "redux-thunk": "^1.0.0",
    "sass-loader": "^1.0.3",
    "sinon": "^1.17.2",
    "style-loader": "^0.13.0",
    "url-loader": "^0.5.6",
    "webpack": "^1.10.5",
    "webpack-dev-server": "^1.10.1"
  },
  "scripts": {
    "clean": "rm -rf build/",
    "start": "parallelshell 'webpack-dev-server --progress' 'npm run build:index:dev'",
    "build": "npm run build:webpack && npm run build:index:prod",
    "build:index": "jade $npm_package_config_jade_in --out $npm_package_config_jade_out",
    "build:index:dev": "npm run build:index -- --obj '{\"hash\":\"build/index\"}' --pretty --watch",
    "build:index:prod": "npm run build:index -- --obj $npm_package_config_webpack_stat",
    "build:webpack": "webpack --progress --json > $npm_package_config_webpack_stat",
    "deploy": "make deploy",
    "test": "mocha --require ./test/config test/*/*.js",
    "cover": "npm run cover:istanbul && istanbul report html && open coverage/index.html",
    "cover:istanbul": "rm -rf ./coverage && istanbul cover _mocha --report json -- --require ./test/config test/*/*.js ",
    "travis": "rm -rf ./coverage && istanbul cover _mocha --report lcovonly -- --require ./test/config test/*/*.js && cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js"
  },
  "config": {
    "jade_in": "src/jade/index.jade",
    "jade_out": "./",
    "webpack_stat": "webpack-stats.json"
  },
  "babel": {
    "optional": [
      "runtime"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/MrOrz/hacktabl.git"
  },
  "author": "MrOrz",
  "license": "MIT",
  "dependencies": {
    "react-dom": "^0.14.2"
  }
}
